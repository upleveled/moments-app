		"catalogs": [
			{
				"path": "locales/{locale}/messages",
				"include": [
					"./pages",
					"./components"
				],
				"exclude": [
					"**/node_modules/**"
				]
			}
		],
		"sourceLocale": "en",
		"locales": [
			"en",
			"es"
		],
		"format": "po"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
